{
  "name": "and-dine-backend",
  "version": "1.0.0",
  "description": "Backend system for &amp;Dine",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "make-model": "npx sequelize-cli model:generate --name user --attributes email:string",
    "make-migration": "npx sequelize-cli migration:generate --name nameModel",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "undo-seed-all": "npx sequelize-cli db:seed:undo:all",
    "undo-seed": "npx sequelize-cli db:seed:undo --seed name-of-seed-as-in-data",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm app.ts"
  },
  "repository": {},
  "author": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0",
    "supervisor": "^0.12.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.5",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1"
  }
}
